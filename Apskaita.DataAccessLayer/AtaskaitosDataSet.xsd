                         saskaita ON mat_vert.SaskaitaId = saskaita.Id LEFT OUTER JOIN
                         padalinys ON mat_vert.PadalinysId = padalinys.Id LEFT OUTER JOIN
                         matovnt ON mat_vert.MatoVntId = matovnt.Id LEFT OUTER JOIN
                         registras ON mat_vert.Id = registras.TurtasId
